{
    "configurations": [
        {
            "name": "Freestanding",
            "includePath": [
                "${workspaceFolder}/kernel/inc",
                "${workspaceFolder}/kernel/cfg"
            ],
            "defines": [
                "__STDC_VERSION__=200710L",
                "nullptr=(void*)0",
                "constexpr=const"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c23",
            "intelliSenseMode": "gcc-x86",
            "compilerArgs": [
                "-ffreestanding",
                "-nostdlib",
                "-lgcc",
                "-std=c23"
            ]
        }
    ],
    "version": 4
}